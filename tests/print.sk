foreign proc printf like print (string) --- ;
foreign proc printf (string int) --- ;

proc main
  "1  " print "%d\n" 1 printf
  "-----------\n" print

  "13  " print "%d\n" 13 printf
  "-----------\n" print

  "666  " print "%d\n" 666 printf
  "-----------\n" print

  "1889  " print "%d\n" 1889 printf
  "-----------\n" print

  "92720  " print "%d\n" 92720 printf
  "-----------\n" print

  "121911  " print "%d\n" 121911 printf
  "-----------\n" print

  "1000000  " print "%d\n" 1_000_000 printf
  "-----------\n" print

  "2000000000  " print "%d\n" 2_000_000_000 printf
;
