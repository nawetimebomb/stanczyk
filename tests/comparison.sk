using "io"

;; Test: comparison
;; Description:
;    Comparing values and checking the results

"equal\n" puts
2 2 = print  ; 1
2 3 = print  ; 0
3 2 = print  ; 0
"-----------\n" puts

"not equal\n" puts
2 3 != print ; 1
3 2 != print ; 1
2 2 != print ; 0
"-----------\n" puts

"less\n" puts
3 2 > print  ; 1
2 3 > print  ; 0
3 3 > print  ; 0
"-----------\n" puts

"less equal\n" puts
3 3 >= print ; 1
3 2 >= print ; 1
2 3 >= print ; 0
"-----------\n" puts

"greater\n" puts
2 3 < print  ; 1
3 2 < print  ; 0
2 2 < print  ; 0
"-----------\n" puts

"greater equal\n" puts
2 2 <= print ; 1
2 3 <= print ; 1
3 2 <= print ; 0
