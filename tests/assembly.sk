using io

fn custom-linux-write (ptr int)
	swap
	asm int ptr (
		mov rax, SYS_WRITE
		mov rdi, STDOUT
		pop rsi
		pop rdx
		syscall
	)
ret

fn return-13 (--- int)
  asm -> int (
		mov rax, 13
		push rax
  )
ret

fn main ()
  "Printing this text with asm\n" dup len custom-linux-write
	return-13 println
ret
